<?xml version="1.0" encoding="utf-8" ?>
<Settings xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://schemas.microsoft.com/2011/01/fabric">
  <!-- This is used by the StateManager's replicator. -->
  <Section Name="ReplicatorConfig">
    <Parameter Name="ReplicatorEndpoint" Value="ReplicatorEndpoint" />
  </Section>
  <!-- This is used for securing StateManager's replication traffic. -->
  <Section Name="ReplicatorSecurityConfig" />

  <Section Name="DefaultServiceConfig">
    <Parameter Name="DefaultMinReplicaSetSize" Value="3"/>
    <Parameter Name="DefaultTargetReplicaSetSize" Value="3"/>
    <Parameter Name="DefaultPartitionCount" Value="1"/>
    <Parameter Name="DefaultPlacementConstraints" Value=""/>
  </Section>

  <Section Name="EventDispatcherPoolConfig">
    <Parameter Name="PoolSize" Value="100"/> <!-- this is default for PROD level and customized for local dev experience-->
  </Section>

</Settings>
